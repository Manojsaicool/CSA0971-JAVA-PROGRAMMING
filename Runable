import java.util.Scanner;
class learnit extends Thread {
   int fact=1;
    @Override
    public void run() {
       Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=1;i<=n;i++){
           fact*=i;
           }
      System.out.println(fact);
    }
}
class threadexam{
public static void main(String[] args){
	learnit Inst = new learnit();
	Inst.start();
   }
}
